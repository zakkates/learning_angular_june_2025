{
  "extends": ["@angular-eslint/eslint-config-angular", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "settings": {
    "angularCompilerOptions": {
      "enableIvy": true
    }
  }
}
